<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />

    <!-- Libs CSS -->

    <link rel="stylesheet" href="../assets/libs/prismjs/themes/prism.css" />
    <link
      rel="stylesheet"
      href="../assets/libs/prismjs/plugins/line-numbers/prism-line-numbers.css"
    />
    <link
      rel="stylesheet"
      href="../assets/libs/prismjs/plugins/toolbar/prism-toolbar.css"
    />
    <link
      rel="stylesheet"
      href="../assets/libs/bootstrap-icons/font/bootstrap-icons.css"
    />
    <link rel="stylesheet" href="../assets/libs/dropzone/dist/dropzone.css" />
    <link
      href="../assets/libs/@mdi/font/css/materialdesignicons.min.css"
      rel="stylesheet"
    />

    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/normalize/8.0.0/normalize.css"
    />

    <!-- Theme CSS -->
    <link rel="stylesheet" href="../assets/css/theme.min.css" />
    <title>Update Game</title>
  </head>

  <body>
    <div id="db-wrapper">
      <!-- navbar vertical -->
      <!-- Sidebar -->
      <nav class="navbar-vertical navbar">
        <div class="nav-scroller">
          <!-- Brand logo -->
          <a class="navbar-brand" href="./index.html">
            <img src="../images/logo.png" alt="" />
            <span class="text-white ps-1"> GAME RPS</span>
          </a>
          <!-- Navbar nav -->
          <ul class="navbar-nav flex-column" id="sideNavbar">
            <li class="nav-item">
              <a class="nav-link has-arrow" href="#">
                <i data-feather="home" class="nav-icon icon-xs me-2"></i>
                Dashboard
              </a>
            </li>

            <!-- Nav item -->
            <li class="nav-item">
              <div class="navbar-heading">Table</div>
            </li>

            <li class="nav-item">
              <a class="nav-link" href="/user-admin">
                <i class="nav-icon icon-xs me-2 bi bi-table"> </i>
                User game
              </a>
            </li>
          </ul>
        </div>
      </nav>
      <!-- Page content -->
      <div id="page-content">
        <div class="header @@classList">
          <!-- navbar -->
          <nav class="navbar-classic navbar navbar-expand-lg">
            <a id="nav-toggle" href="#"
              ><i data-feather="menu" class="nav-icon me-2 icon-xs"></i
            ></a>

            <!--Navbar nav -->
            <ul
              class="navbar-nav navbar-right-wrap ms-auto d-flex nav-top-wrap"
            >
              <!-- List -->
              <li class="dropdown ms-2">
                <a
                  class="rounded-circle"
                  href="#"
                  role="button"
                  id="dropdownUser"
                  data-bs-toggle="dropdown"
                  aria-haspopup="true"
                  aria-expanded="false"
                >
                  <div class="avatar avatar-md avatar-indicators avatar-online">
                    <img
                      alt="avatar"
                      src="../assets/images/avatar/avatar-1.jpg"
                      class="rounded-circle"
                    />
                  </div>
                </a>
                <div
                  class="dropdown-menu dropdown-menu-end"
                  aria-labelledby="dropdownUser"
                >
                  <div class="px-4 pt-2 pb-0">
                    <div class="lh-1">
                      <h5 class="mb-1">John E. Grainger</h5>
                      <a href="#" class="text-inherit fs-6">View my profile</a>
                    </div>
                    <div class="mt-3 mb-2 dropdown-divider"></div>
                  </div>

                  <ul class="list-unstyled">
                    <li>
                      <a class="dropdown-item" href="#">
                        <i
                          class="me-2 icon-xxs dropdown-item-icon"
                          data-feather="user"
                        ></i
                        >Edit Profile
                      </a>
                    </li>

                    <li>
                      <a class="dropdown-item" href="./index.html">
                        <i
                          class="me-2 icon-xxs dropdown-item-icon"
                          data-feather="power"
                        ></i
                        >Sign Out
                      </a>
                    </li>
                  </ul>
                </div>
              </li>
            </ul>
          </nav>
        </div>
        <!-- Container fluid -->
        <div class="pt-10 bg-primary pb-21"></div>
        <div class="px-6 container-fluid mt-n22">
          <div class="row">
            <div class="col-lg-12 col-md-12 col-12">
              <!-- Page header -->
              <div>
                <div class="d-flex justify-content-between align-items-center">
                  <div class="mb-2 mb-lg-0">
                    <h3 class="mb-0 text-white fw-bold">User Game Biodata</h3>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <!-- row  -->
          <div class="mt-6 row">
            <div class="col-md-12 col-12">
              <!-- card  -->
              <div class="card">
                <!-- card header  -->
                <div class="py-4 bg-white card-header border-bottom-0">
                  <h4 class="mb-0">Form Update User Biodata</h4>
                </div>
                <!-- table  -->
                <div class="p-4 tab-content">
                  <form action="/user-admin/update" method="POST">
                    <div class="mb-3 row">
                      <label for="Username" class="col-sm-2 col-form-label"
                        >Usernamse
                      </label>

                      <div class="col-sm-10">
                        <input
                          type="text"
                          class="form-control"
                          name="username"
                          id="Username"
                          value="<%= getGameBiodata.user_biodata.username %>"
                        />
                      </div>
                    </div>

                    <div class="mb-3 row">
                      <label for="Email" class="col-sm-2 col-form-label"
                        >Email</label
                      >
                      <div class="col-sm-10">
                        <input
                          type="email"
                          class="form-control"
                          id="Email"
                          name="email"
                          value="<%= getGameBiodata.user_biodata.email %>"
                        />
                      </div>
                    </div>
                    <input
                      type="hidden"
                      name="id"
                      value="<%= getGameBiodata.user_biodata.id %>"
                    />
                    <input
                      type="hidden"
                      name="idx"
                      value="<%= getGameBiodata.id %>"
                    />
                    <input
                      type="hidden"
                      name="password"
                      value="<%= getGameBiodata.user_biodata.password %>"
                    />
                    <div class="mb-3 row">
                      <label for="nama" class="col-sm-2 col-form-label"
                        >Nama lengkap</label
                      >
                      <div class="col-sm-10">
                        <input
                          type="text"
                          class="form-control"
                          name="nama"
                          id="nama"
                          value="<%= getGameBiodata.nama %>"
                        />
                      </div>
                    </div>
                    <div class="mb-3 row">
                      <label for="tanggal_lahir" class="col-sm-2 col-form-label"
                        >Tanggal Lahir</label
                      >
                      <div class="col-sm-10">
                        <input
                          type="date"
                          class="form-control"
                          name="tanggal_lahir"
                          id="tanggal_lahir"
                          value="<%= getGameBiodata.tanggal_lahir %>"
                        />
                      </div>
                    </div>
                    <div class="mb-3 row">
                      <label for="alamat" class="col-sm-2 col-form-label"
                        >Alamat</label
                      >
                      <div class="col-sm-10">
                        <input
                          type="text"
                          class="form-control"
                          name="alamat"
                          id="alamat"
                          value="<%= getGameBiodata.alamat %>"
                        />
                      </div>
                    </div>
                    <div class="mb-3 row">
                      <label for="notelp" class="col-sm-2 col-form-label"
                        >No Telpon</label
                      >
                      <div class="col-sm-10">
                        <input
                          type="text"
                          class="form-control"
                          name="notelp"
                          id="notelp"
                          value="<%= getGameBiodata.notelp %>"
                        />
                      </div>
                    </div>
                    <div class="mb-3 row">
                      <input
                        type="submit"
                        class="btn btn-primary"
                        value="Update"
                      />
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Scripts -->
    <!-- Libs JS -->
    <script src="../assets/libs/jquery/dist/jquery.min.js"></script>
    <script src="../assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="../assets/libs/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <script src="../assets/libs/feather-icons/dist/feather.min.js"></script>
    <script src="../assets/libs/prismjs/components/prism-core.min.js"></script>
    <script src="../assets/libs/prismjs/components/prism-markup.min.js"></script>
    <script src="../assets/libs/prismjs/plugins/line-numbers/prism-line-numbers.min.js"></script>
    <script src="../assets/libs/apexcharts/dist/apexcharts.min.js"></script>
    <script src="../assets/libs/dropzone/dist/min/dropzone.min.js"></script>

    <!-- clipboard -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/clipboard.js/1.5.12/clipboard.min.js"></script>

    <!-- Theme JS -->
    <script src="../assets/js/theme.min.js"></script>
  </body>
</html>
